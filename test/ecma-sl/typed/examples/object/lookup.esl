function main() {
	objSimple: { foo: int, bar: string } := { foo: 10, bar: "abc" };
	objComplex: { foo: { bar: string } } := { foo: { bar: "abc" } };
	objComplexUndef: { foo: undefined } := { foo: 'undefined };
	objUndef: undefined := 'undefined;
	objNull: null := null;
	objUnknown: unknown := 10;
	objFalse: int := 10;

	x: int := objSimple.foo;						/* Valid */
	y: string := objSimple.bar;					/* Valid */
	z: string := objComplex.foo.bar;		/* Valid */
	
	x: int := objSimple.bar;						/* BadValue: int <- string */
	y: int := objSimple.baz;						/* BadLookup: obj.baz */
	z: int := objComplex.foo.bar;				/* BadLookup: int <- string */

	objComplexUndef.foo.bar;		/* BadPossibleType: objComplexUndef.foo <- undefined */
	objUndef.foo;								/* BadPossibleType: undef_obj <- undefined */
	objNull.foo;								/* BadPossibleType: null_obj <- null */
	objUnknown.foo;							/* BadType: unknown_obj <- unknown */
	objFalse.foo;								/* BadLookup: false_obj.foo */
	return 0
}