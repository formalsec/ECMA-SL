function main() {
	objSimple: { foo: int, bar: string } := { foo: 10, bar: "abc" };
	objComplex: { foo: { bar: string } } := { foo: { bar: "abc" } };
	objComplexUndef: { foo: undefined } := { foo: 'undefined };
	objUndef: undefined := 'undefined;
	objNull: null := null;
	objUnknown: unknown := 10;
	objFalse: int := 10;

	objSimple.foo := 10;								/* Valid */
	objSimple.bar := "abc";							/* Valid */
	objComplex.foo.bar := "abc";				/* Valid */
	
	objSimple.bar := 10;								/* BadValue: string <- int */
	objSimple.baz := 10;								/* BadLookup: obj.baz */
	objComplex.foo.bar := 10;						/* BadValue: string <- int */

	objComplexUndef.foo.bar := "abc";		/* BadPossibleType: objComplexUndef.foo <- undefined */
	objUndef.foo := 10;									/* BadPossibleType: undef_obj <- undefined */
	objNull.foo := 10;									/* BadPossibleType: null_obj <- null */
	objUnknown.foo := 10;								/* BadType: unknown_obj <- unknown */
	objFalse.foo := 10;									/* BadLookup: false_obj.foo */
	return 0
}