function main() {
	x: {} := {};																								/* Valid */
	y: { foo: int } := { foo: 10 };															/* Valid */
	z: { foo: int, bar: string } := { foo: 10, bar: "abc" };		/* Valid */

	x: { foo: int } := { foo: 10, foo: 20 };										/* DuplicatedField: foo */
	z: { foo: int } := { foo: 10, bar: "abc" };									/* ExtraField: bar */
	x: { foo: int, bar: string } := { foo: 10 };								/* MissingField: bar */
	w: { foo: int, bar: string } := { foo: 10, bar: true };			/* BadField(bar): string <- boolean */
	
	obj: { foo: int, bar: string } := { foo: 10, bar: "abc" };
	x: { foo: int, bar: string, baz: boolean } := obj;					/* MissingField: baz */
	y: { foo: int, bar: boolean } := obj;												/* BadField(bar): boolean <- string */
	return 0
}