function main() {
  anyVariable: any := 10;                       /* Valid */
	unknownVariable: unknown := 10;               /* Valid */
	undefinedVariable: undefined := 'undefined;   /* Valid */

  foo: string := anyVariable;                   /* Valid */
  baz: int := unknownVariable;                  /* BadValue: int <- unknown */
	bar: never := 10;                             /* BadValue: never <- int */
  qux: symbol := 'undefined;                    /* BadValue: symbol <- undefined */ 
  return 0
}
