function main() {
	literalInt: 10 := 10;							/* Valid */
	literalFlt: 10.1 := 10.1;					/* Valid */
	literalString: "abc" := "abc";		/* Valid */
	literalBoolean: true := true;			/* Valid */
	literalSymbol: 'a := 'a;					/* Valid */
