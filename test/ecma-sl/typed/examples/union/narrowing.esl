function _unionFunc()	: int | string { return 10 };

function main() {
	foo: int | string := 10;
	bar: int | string := _unionFunc();
	baz: int | any := 10;
	qux: int | never := 10;
	fred: int | unknown := 10;

	x: int := foo;  			/* Valid */
	y: string := foo;  		/* BadValue: string <- int */
	z: int := bar;  			/* BadValue: int <- string */
	w: string := bar;  		/* BadValue: string <- int */

	x: string := baz;			/* Valid */
	y: int := qux;				/* Valid */
	z: int := fred;				/* BadValue: int <- unknown */
	return 0
}