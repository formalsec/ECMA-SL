typedef type_t := { foo: int, bar: type_t | null };

function main() {
	x: type_t := { foo: 10, bar: null };											/* Valid */
	y: type_t := { foo: 10, bar: { foo: 20, bar: null } };		/* Valid */
	z: type_t := { foo: 10, bar: x };													/* Valid */

	w: int := x.foo;																					/* Valid */
	v: null := x.bar;																					/* BadValue: null <- type_t */
	x.bar := z;																								/* Valid */
	x.bar.bar := y;																						/* BadPossibleType: x.bar <- null */

	return 0
}