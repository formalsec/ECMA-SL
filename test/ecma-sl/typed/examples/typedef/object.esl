typedef object_t := { foo: int, bar: string };

function main() {
	x: object_t := { foo: 10, bar: "abc" };				/* Valid */
	y: int := x.foo;															/* Valid */
	z: int := x.bar;															/* BadValue: int <- string */
	
	x.foo := 20;																	/* Valid */
	x.bar := 20;																	/* BadValue: string <- int */

	w: fakeObject_t := { foo: 10, bar: "abc" };		/* UnknownType: fakeObject_t */
	v: object_t := { foo: 10, bar: 20 };					/* BadValue: string <- int */

	return 0
}