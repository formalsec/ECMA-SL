function _sigma() : sigma[type] { type: "foo", foo: int } | { type: "bar" } {
	return { type: "foo", foo: 10 }
};

function main() {
	x: sigma[type] { type: "foo", foo: int } | { type: "bar" } := _sigma();

	match x with
	| default -> {};						/* Valid */

	match x with
	| { type: "foo" } -> {}
	| default -> {};						/* Valid */

	match x with
	| { type: "foo" } -> {}
	| { type: "bar" } -> {}
	| default -> {};						/* UnusedPatternCase */

	match x with
	| default -> {}							/* Valid */
	| { type: "foo" } -> {}			/* UnusedPatternCase */
	| default -> {};						/* UnusedPatternCase */

	return 0
}