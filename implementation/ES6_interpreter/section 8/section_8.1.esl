    existingProp := HasOwnProperty(globalObject, N);

    /* ReturnIfAbrupt(existingProp). */
    @ReturnIfAbrupt(existingProp);

    /* If existingProp is true, then */
    if ( existingProp = true ) {
        /* Let status be ObjRec.DeleteBinding(N). */
        status := {ObjRec.DeleteBinding}(ObjRec, N);

